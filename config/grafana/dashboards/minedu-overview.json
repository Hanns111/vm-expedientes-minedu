{
  "dashboard": {
    "id": null,
    "title": "MINEDU - Sistema de Análisis de Documentos",
    "description": "Dashboard principal para monitoreo del sistema MINEDU",
    "tags": ["minedu", "performance", "monitoring"],
    "timezone": "America/Lima",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Estado del Sistema",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"minedu-backend\"}",
            "refId": "A",
            "legendFormat": "Backend"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [
              {
                "options": {
                  "0": {"text": "DOWN", "color": "red"},
                  "1": {"text": "UP", "color": "green"}
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Tiempo de Respuesta (95th percentile)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(minedu_http_request_duration_seconds_bucket[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.5},
                {"color": "red", "value": 2.0}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Modelos Cargados",
        "type": "stat",
        "targets": [
          {
            "expr": "minedu_models_loaded",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 3}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Tasa de Error",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(minedu_http_requests_total{status=~\"5..\"}[5m]) / rate(minedu_http_requests_total[5m]) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 15}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Latencia de Búsquedas",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(minedu_search_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(minedu_search_duration_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(minedu_search_duration_seconds_bucket[5m]))",
            "refId": "C",
            "legendFormat": "p99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4}
      },
      {
        "id": 6,
        "title": "Requests por Minuto",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(minedu_http_requests_total[1m]) * 60",
            "refId": "A",
            "legendFormat": "{{ method }} {{ endpoint }}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4}
      },
      {
        "id": 7,
        "title": "Uso de CPU",
        "type": "timeseries",
        "targets": [
          {
            "expr": "minedu_system_cpu_percent",
            "refId": "A",
            "legendFormat": "CPU %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            },
            "max": 100,
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 12}
      },
      {
        "id": 8,
        "title": "Uso de Memoria",
        "type": "timeseries",
        "targets": [
          {
            "expr": "minedu_system_memory_percent",
            "refId": "A",
            "legendFormat": "Memory %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 95}
              ]
            },
            "max": 100,
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 12}
      },
      {
        "id": 9,
        "title": "Memoria de Modelos ML",
        "type": "timeseries",
        "targets": [
          {
            "expr": "minedu_model_memory_bytes / (1024*1024*1024)",
            "refId": "A",
            "legendFormat": "{{ model_name }}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "decbytes",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 12}
      },
      {
        "id": 10,
        "title": "Distribución de Métodos de Búsqueda",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (method) (rate(minedu_search_requests_total[5m]))",
            "refId": "A",
            "legendFormat": "{{ method }}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 11,
        "title": "Tiempo de Inferencia de Modelos",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(minedu_model_inference_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "{{ model_name }} (p95)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20}
      },
      {
        "id": 12,
        "title": "Alertas Activas",
        "type": "table",
        "targets": [
          {
            "expr": "ALERTS{alertstate=\"firing\"}",
            "refId": "A",
            "format": "table"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "job": true,
                "instance": true
              },
              "indexByName": {},
              "renameByName": {
                "alertname": "Alert",
                "severity": "Severity",
                "summary": "Summary"
              }
            }
          }
        ],
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 28}
      }
    ],
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "editable": true,
    "gnetId": null,
    "graphTooltip": 0,
    "links": [],
    "panels": [],
    "schemaVersion": 27,
    "style": "dark",
    "templating": {
      "list": []
    },
    "timepicker": {},
    "version": 1
  },
  "folderId": null,
  "overwrite": false
}